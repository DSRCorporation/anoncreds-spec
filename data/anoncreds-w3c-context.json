{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "ac": "https://anoncreds.example/2022/ns#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "AnonCredsCredential": "ac:AnonCredsCredential",

    "AnonCredsDefinition": {
      "@id": "ac:AnonCredsDefinition",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "definition": {
          "@id": "ac:definition",
          "@type": "@id"
        },
        "schema": {
          "@id": "ac:schema",
          "@type": "@id"
        },
        "revocationRegistry": {
          "@id": "ac:revocationRegistry",
          "@type": "@id"
        },
        "encoding": {
          "@id": "ac:encoding",
          "@type": "@vocab",
          "@context": {
            "@version": 1.1,
            "@protected": true,
            "auto": "ac:autoEncoding"
          }
        }
      }
    },

    "AnonCredsPresentation": "ac:AnonCredsPresentation",

    "AnonCredsPresentationProof2023": {
      "@id": "ac:AnonCredsPresentationProof2023",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "mapping": {
          "@id": "ac:mapping",
          "@type": "@json"
        },
        "proofValue": {
          "@id": "ac:proofValue",
          "@type": "xsd:string"
        },
        "challenge": {
          "@id": "ac:challenge",
          "@type": "xsd:string"
        }
      }
    },

    "CLSignature2023": {
      "@id": "ac:CLSignature2023",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "signature": {
          "@id": "ac:signature",
          "@type": "xsd:string"
        }
      }
    },

    "AnonCredsPredicate": {
      "@id": "ac:AnonCredsPredicate",
      "@context": {
        "@version": 1.1,
        "@protected": true,
        "p_type": {
          "@id": "ac:p_type",
          "@type": "xsd:string"
        },
        "p_value": {
          "@id": "ac:p_value",
          "@type": "xsd:decimal"
        }
      }
    }
  }
}